<link rel='stylesheet' href='/stylesheets/social_activities.css' />

<div id="main" style="margin-bottom:400px;">
	<div class="row">
        <div class="col s12 m12 l12" id="titleHead">
        	<h2 id="titleh2">Social Activities</h2>
        </div>
{{# each socials}}
		    <div class="col s12 m12 l4" style="margin-bottom:10px;">
            <div class="hoverable z-depth-5 cyan lighten-5"> 
                <img class="materialboxed" id="socialImg" data-caption="{{this.socialdesc}}"  src="../{{this.socialupload}}" style="width:100%;"> 
                    <div class="center-align cyan lighten-5">
                        <h5 id="fonth5" class="cyan-text text-darken-2 truncate" style="padding-bottom:15px;">{{this.socialtitle}}</h5>
                    </div>
            </div>
            </div> 
{{/each}}

	</div>
</div>

<script type="text/javascript">
 $(document).ready(function(){
    $('.materialboxed').materialbox();
  });
</script>