        	    <div class="aftrcvr">
    	        	  <div class="row">
                        <div class="col s12 m12 l12">
                        	<h4 class="deep-purple-text text-lighten-1"><b>Most Popular Videos</b></h4>
                        </div>
                        {{# each populars}}
                        <div class="col s12 m12 l4">
                          <div class=" blue-grey darken-1" style="width:100%; margin-top:20px;">
                              <iframe width="100%" height="40%" src="https://www.youtube.com/embed/{{this.pvideourl}}" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>
                            <div class="cczjn">
                              <h5 class="center-align" style="padding:5px; font-size:20px; line-height: 1.6;"><a class="white-text truncate" href="#">{{this.pvideotitle}}</a></h5>
                              <span><h6 class="center-align truncate" style="padding:3px; font-size:16px;"><a class="white-text" href="http://utei.in/">Created By {{this.pvideocreatedby}}</a></h6></span>
                            </div>
                          </div>
                        </div>
                        {{/each}}
                  </div>
