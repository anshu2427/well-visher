            </div>
        
        <div id="Dance" class="col s12 m12 l12"><div class="hide-on-med-and-down"><br><br></div>	
        	 <h3 class="red white-text" style="padding-left:30px; font-size:30;">Dance Create Happiness <span><h6 class="right-align" style="padding-left:48%; padding-right:0.2%; display:inline-block; font-size:24px;"><a href="/dance" class="white-text"> see more<i class="material-icons">navigate_next</i></a></h6></span></h3>
            <div class="row">	
              {{# each dances}}
    	          <div class="col s12 m12 l4">
                   <div class=" blue-grey darken-1" style="width:100%; margin-top:20px;">
                      <iframe width="100%" height="40%" src="{{this.dvideourl}}" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>
                    <div class="cczjn">
                       <h5 class="center-align truncate" style="padding:5px; font-size:20px; line-height: 1.6;"><a class="white-text" href="#">{{this.dvideotitle}}</a></h5>
                       <span><h6 class="center-align truncate" style="padding:3px;"><a class="white-text" href="http://utei.in/">Created By {{this.dvideocreatedby}}</a></h6></span>
                     </div>
                   </div>
                </div> 
              {{/each}}  
            </div>
        </div>    
        

        <div id="Social" class="col s12 m12 l12"><div class="hide-on-med-and-down"><br><br></div>
        	 <h3 class="red white-text" style="padding-left:30px; font-size:30;">Social Activities <span><h6 class="right-align" style="padding-left:61%; padding-right:0.2%; display:inline-block; font-size:24px;"><a href="/social" class="white-text"> see more<i class="material-icons">navigate_next</i></a></h6></span></h3>
            <div class="row">	
              {{# each socials }}
    	          <div class="col s12 m12 l4" style="margin-bottom:10px;">
    	        	    <div class="hoverable z-depth-5 cyan lighten-5"> 
                              <img class="materialboxed" data-caption="{{this.socialdesc}}"  src="../{{this.socialupload}}" style="width:100%; height:500px;">
                              <div class="center-align cyan lighten-5">
                                  <h5 class="cyan-text text-darken-2 truncate" style="font-size:18px; padding-bottom:15px;">{{this.socialtitle}}</h5>
                              </div>
  	                </div>
                </div>
              {{/each}}  
            </div>
        </div>

        <div id="Picture" class="col s12 m12 l12"><div class="hide-on-med-and-down"><br><br></div>
        	 <h3 class="red white-text" style="padding-left:30px; font-size:30;">Picture Gallery <span><h6 class="right-align" style="padding-left:62%; padding-right:0.2%; display:inline-block; font-size:24px;"><a href="/picture" class="white-text"> see more<i class="material-icons">navigate_next</i></a></h6></span></h3>
            <div class="row">	
             {{# each pictures}} 
    	        <div class="col s12 m12 l4" style="margin-bottom:10px;">
    	        	<div class="hoverable z-depth-5 cyan lighten-5"> 
                              <img class="materialboxed" data-caption="{{this.picturedesc}}"  src="../{{this.pictureupload}}" style="width:100%; height:500px;"> 
                              <div class="center-align cyan lighten-5">
                                  <h5 class="cyan-text text-darken-2 truncate" style="font-size:18px; padding-bottom:15px;">{{picturetitle}}</h5>
                              </div>
  	                </div>
                </div>
             {{/each}}
            </div>
        </div>

        
        <div id="Media" class="col s12 m12 l12"><div class="hide-on-med-and-down"><br><br></div>
        	  <h3 class="red white-text" style="padding-left:30px; font-size:30;">Media <span><h6 class="right-align" style="padding-left:62%; padding-right:0.2%; display:inline-block; font-size:24px;"><a href="#Media" class="red-text"> see more<i class="material-icons">navigate_next</i></a></h6></span></h3>
